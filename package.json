{
  "private": true,
  "scripts": {
    "build": "npm run build-webpack && npm run build-static-storybook && npm run build-html",
    "build-webpack": "NODE_ENV=production webpack --progress --hide-modules",
    "build-html": "babel-node src/build-html.js",
    "build-static-storybook": "build-storybook -c .storybook -o build/storybook",
    "deploy": "./scripts/deploy.sh",
    "build+deploy": "npm run build && npm run deploy",
    "watch": "webpack-dev-server --content-base build/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "bootstrap": "^3.3.7",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.0-beta",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "ignore-styles": "^5.0.1",
    "markdown-loader": "^2.0.0",
    "marked": "^0.3.6",
    "node-sass": "^4.5.0",
    "require-hacker": "^3.0.0",
    "sass-loader": "^6.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
